    <content><![CDATA[<link rel="stylesheet" class="aplayer-secondary-style-marker" href="\assets\css\APlayer.min.css"><script src="\assets\js\APlayer.min.js" class="aplayer-secondary-script-marker"></script><h1 id="01背包问题"><a href="#01背包问题" class="headerlink" title="01背包问题"></a>01背包问题</h1><p><a href="https://www.acwing.com/problem/content/2/">题目链接</a></p>
<figure class="highlight plain"><table><tr><td class="code"><pre><span class="line">#include &lt;iostream&gt;</span><br><span class="line">#include&lt;algorithm&gt;</span><br><span class="line">#include &lt;cstring&gt;</span><br><span class="line">using namespace std;</span><br><span class="line">#define MAX 1005</span><br><span class="line">#define M 1005</span><br><span class="line">int c[M][MAX]; </span><br><span class="line">int w[M],v[M];  </span><br><span class="line">int x[M];       </span><br><span class="line">int main()&#123;</span><br><span class="line">    int i,j,n,W;  </span><br><span class="line">    cin&gt;&gt;n&gt;&gt;W;</span><br><span class="line">    for(i&#x3D;1;i&lt;&#x3D;n;i++)&#123;</span><br><span class="line">        cin&gt;&gt;w[i]&gt;&gt;v[i];</span><br><span class="line">    &#125;</span><br><span class="line">    for(i&#x3D;0;i&lt;&#x3D;n;i++)&#123; </span><br><span class="line">        c[i][0]&#x3D;0;</span><br><span class="line">    &#125;</span><br><span class="line">    for(j&#x3D;0;j&lt;&#x3D;W;j++)&#123; </span><br><span class="line">        c[0][j]&#x3D;0;</span><br><span class="line">    &#125;</span><br><span class="line">    for(i&#x3D;1;i&lt;&#x3D;n;i++)&#123;</span><br><span class="line">        for(j&#x3D;1;j&lt;&#x3D;W;j++)&#123;</span><br><span class="line">            if(j&lt;w[i])</span><br><span class="line">              c[i][j]&#x3D;c[i-1][j];</span><br><span class="line">            else</span><br><span class="line">              c[i][j]&#x3D;max(c[i-1][j],c[i-1][j-w[i]]+v[i]);</span><br><span class="line">        &#125;</span><br><span class="line">    &#125;</span><br><span class="line">    cout &lt;&lt; c[n][W] &lt;&lt; endl;</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>
]]></content>
